execute as @e[scores={ifPower=16}] at @s run summon area_effect_cloud ~ ~1.3 ~ {CustomNameVisible:1b,Radius:0f,Duration:1,WaitTime:0,Tags:["inPowerDisplay"],CustomName:'{"text":"16 IF","color":"red","italic":false}'}
